#! /bin/sh
#
# src/libst-config.  Generated from libst-config.in by configure.
#

prefix="/usr"
exec_prefix="/usr"
includedir="/usr/include"
libdir="/usr/lib"
libs='-lvorbisenc -lvorbisfile -logg -lasound -lm '

case "$1" in
--prefix)
        echo $prefix;;
--includedir)
        echo $includedir;;
--libdir)
        echo $libdir;;
--libs)
        echo $libs;;
--cflags)
        echo "-L'$libdir' -I'$includedir'";;
*)
        echo "Usage: $0 [--prefix|--includedir|--libdir|--cflags|--libs]"
        exit 1;;
esac

exit 0
